{
  "name": "server",
  "version": "0.2.1",
  "main": "src/server.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node-modules --respawn src/server.ts",
    "knex:migrate": "knex --knexfile src/config/knexfile.ts migrate:latest",
    "knex:rollback": "knex --knexfile src/config/knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "@sentry/node": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.2",
    "mysql": "^2.18.1",
    "sqlite3": "^5.0.0"
  }
}
